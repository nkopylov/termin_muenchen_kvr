# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Admin Telegram ID (optional - for health check alerts)
# To get your ID, message @userinfobot on Telegram
ADMIN_TELEGRAM_ID=your_telegram_user_id_here

# Appointment Check Interval (optional - default: 2 minutes)
# How often to check for new appointments (minimum: 5, maximum: 600)
# Lower values = faster notifications but more API calls
CHECK_INTERVAL=120

# Analytics Configuration
ANALYTICS_ENABLED=true
UMAMI_ENDPOINT=http://umami:3000
UMAMI_WEBSITE_ID=your-website-uuid-from-umami
